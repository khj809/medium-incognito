{
    "manifest_version" : 2,
    "name": "Medium Incognito",
    "description" : "Open Medium posts on Incognito windows",
    "version" :"0.1",

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions" : [
        "tabs",
        "webNavigation",
        "<all_urls>"
    ],
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    }
}
